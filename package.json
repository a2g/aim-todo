    "@types/response-time": "^2.3.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "concurrently": "^5.3.0",
    "coveralls": "^3.0.2",
    "cspell": "^8.17.1",
    "eslint": "^7.32.0",
    "jest": "^29.5.0",
    "prompt-sync": "^4.2.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^7.0.1",
    "ts-standard": "^11.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
