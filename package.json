    "jsonc-parser": "^3.2.0",
    "redis": "3.0.2",
    "response-time": "^2.3.2",
    "standard": "^17.1.0",
    "svgdom": "^0.1.14",
    "xmlbuilder2": "^3.1.0",
    "xmldom": "^0.6.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "customElements": "custom-elements.json",
  "ts-standard": {
    "ignore": [
      "src/**/*.ts"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "ts-standard --fix"
    ]
  }
